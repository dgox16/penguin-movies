{
    "linter": {
        "enabled": true,
        "rules": {
            "all": true,
            "nursery": {
                "all": false
            },
            "complexity": {
                "noForEach": "off"
            }
        }
    },
    "formatter": {
        "formatWithErrors": false,
        "indentStyle": "space",
        "indentWidth": 4,
        "lineWidth": 90
    },
    "organizeImports": {
        "enabled": true
    }
}
